# Example .env file for local development
# Copy this file to .env and fill in your values
# DO NOT commit the real .env file to version control

# Client credentials for API authentication
MEMVERSE_CLIENT_ID=your_client_id_here
MEMVERSE_CLIENT_API_KEY=your_client_secret_here

# Optional test user credentials (for development only)
MEMVERSE_USERNAME=your_test_username_here
MEMVERSE_CORRECT_PASSWORD_DO_NOT_COMMIT=your_test_password_here

# Testing configuration
TESTER_TYPE=local_development